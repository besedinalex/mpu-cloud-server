{
  "name": "mpu-cloud-server",
  "version": "0.1.0",
  "private": true,
  "bin": "src/index.js",
  "scripts": {
    "start": "nodemon src/index.js",
    "test": "mocha --recursive tests/**/*.spec.js",
    "sqlite-win-x86": "npm i sqlite3-simple-api --target_arch=ia32",
    "sqlite-win-x64": "npm i sqlite3-simple-api --target_arch=x64",
    "build-win-x86": "pkg . --targets=node12-win-x86",
    "build-win-x64": "pkg . --targets=node12-win-x64"
  },
  "pkg": {
    "scripts": [],
    "assets": [
      "public/**/*"
    ]
  },
  "dependencies": {
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-validator": "^6.6.0",
    "fs-extra": "^8.1.0",
    "generate-password": "^1.5.1",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.11",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "request": "^2.88.0",
    "sqlite3-simple-api": "0.0.5"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^7.2.0",
    "nodemon": "^2.0.4"
  }
}
